<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Appareil" table="appareil">
    <id name="idAppareil" type="integer" column="id_appareil">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomAppareil" type="string" column="nom_appareil" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="typeAppareil" type="string" column="type_appareil" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numeroSerie" type="string" column="numero_serie" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imei" type="string" column="imei" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="commune" type="string" column="commune" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statutAppareil" type="string" column="statut_appareil" length="0" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">actif</option>
      </options>
    </field>
    <field name="dateEnregistrement" type="date" column="date_enregistrement" nullable="true"/>
  </entity>
</doctrine-mapping>
