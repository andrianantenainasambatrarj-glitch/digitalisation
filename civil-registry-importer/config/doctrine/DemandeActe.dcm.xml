<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DemandeActe" table="demande_acte">
    <indexes>
      <index name="id_agent_validateur" columns="id_agent_validateur"/>
      <index name="id_appareil" columns="id_appareil"/>
      <index name="id_personne" columns="id_personne"/>
      <index name="id_statut" columns="id_statut"/>
    </indexes>
    <id name="idDemande" type="integer" column="id_demande">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typeActe" type="string" column="type_acte" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idPersonne" type="integer" column="id_personne" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateDemande" type="datetime" column="date_demande" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="idAppareil" type="integer" column="id_appareil" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idStatut" type="integer" column="id_statut" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="commentaire" type="string" column="commentaire" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idAgentValidateur" type="integer" column="id_agent_validateur" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateValidation" type="datetime" column="date_validation" nullable="true"/>
    <field name="idActeLie" type="integer" column="id_acte_lie" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
