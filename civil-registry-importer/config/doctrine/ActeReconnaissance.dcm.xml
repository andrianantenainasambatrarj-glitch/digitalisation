<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ActeReconnaissance" table="acte_reconnaissance">
    <indexes>
      <index name="id_demande" columns="id_demande"/>
      <index name="id_enfant" columns="id_enfant"/>
      <index name="id_officier" columns="id_officier"/>
      <index name="id_reconnaissant" columns="id_reconnaissant"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="num_acte" columns="num_acte"/>
    </unique-constraints>
    <id name="idReconnaissance" type="integer" column="id_reconnaissance">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numActe" type="string" column="num_acte" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idDemande" type="integer" column="id_demande" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idEnfant" type="integer" column="id_enfant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idReconnaissant" type="integer" column="id_reconnaissant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateReconnaissance" type="date" column="date_reconnaissance" nullable="false"/>
    <field name="idOfficier" type="integer" column="id_officier" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commentaire" type="string" column="commentaire" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
