<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AgentEtatCivil" table="agent_etat_civil">
    <unique-constraints>
      <unique-constraint name="login" columns="login"/>
    </unique-constraints>
    <id name="idAgent" type="integer" column="id_agent">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fonction" type="string" column="fonction" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="motDePasse" type="string" column="mot_de_passe" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
