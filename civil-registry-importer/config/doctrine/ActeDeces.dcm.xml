<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ActeDeces" table="acte_deces">
    <indexes>
      <index name="id_conjoint" columns="id_conjoint"/>
      <index name="id_declarant" columns="id_declarant"/>
      <index name="id_defunt" columns="id_defunt"/>
      <index name="id_demande" columns="id_demande"/>
      <index name="id_mere" columns="id_mere"/>
      <index name="id_officier" columns="id_officier"/>
      <index name="id_pere" columns="id_pere"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="num_acte" columns="num_acte"/>
    </unique-constraints>
    <id name="idDeces" type="integer" column="id_deces">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numActe" type="string" column="num_acte" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idDemande" type="integer" column="id_demande" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idDefunt" type="integer" column="id_defunt" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idConjoint" type="integer" column="id_conjoint" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idPere" type="integer" column="id_pere" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idMere" type="integer" column="id_mere" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateDeces" type="date" column="date_deces" nullable="false"/>
    <field name="heureDeces" type="time" column="heure_deces" nullable="true"/>
    <field name="lieuDeces" type="string" column="lieu_deces" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idDeclarant" type="integer" column="id_declarant" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idOfficier" type="integer" column="id_officier" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateActe" type="date" column="date_acte" nullable="false"/>
    <field name="dateDelivrance" type="date" column="date_delivrance" nullable="true"/>
    <field name="mentionMarginale" type="string" column="mention_marginale" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
