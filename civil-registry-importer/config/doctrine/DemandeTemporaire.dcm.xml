<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DemandeTemporaire" table="demande_temporaire">
    <indexes>
      <index name="id_appareil" columns="id_appareil"/>
      <index name="id_statut" columns="id_statut"/>
    </indexes>
    <id name="idTemp" type="integer" column="id_temp">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typeActe" type="string" column="type_acte" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contenuJson" type="text" column="contenu_json" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idAppareil" type="integer" column="id_appareil" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateSaisie" type="datetime" column="date_saisie" nullable="true">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="idStatut" type="integer" column="id_statut" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">1</option>
      </options>
    </field>
    <field name="commentaire" type="string" column="commentaire" length="150" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
